# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Service Role Key (Required for bulk user creation script)
# WARNING: Keep this secret! Never commit to git or expose to frontend
# Get this from: Supabase Dashboard > Project Settings > API > service_role key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Application Configuration
VITE_APP_NAME=CollabSpace
VITE_APP_URL=http://localhost:5173

# File Upload Limits
VITE_MAX_FILE_SIZE=10485760
# 10MB in bytes (10 * 1024 * 1024)

# Allowed File Types
VITE_ALLOWED_FILE_TYPES=application/pdf,application/vnd.ms-powerpoint,application/vnd.openxmlformats-officedocument.presentationml.presentation,application/x-ipynb+json,application/zip,application/x-zip-compressed
